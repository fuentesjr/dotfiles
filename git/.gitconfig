[url "https://github.com/"]
        insteadOf = git@github.com:
[credential]
        helper = cache
[user]
  name = Salvador Fuentes Jr.
  email = fuentesjr@github.com

[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  dfc = diff --cached
  lg = log -p
  last = log -n 1
  graph = log --graph
  dec = log --pretty=oneline --decorate
  sub = submodule

[color]
  branch = auto
  diff = auto
  status = auto

[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green

[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[merge]
  tool = vimdiff
  conflictstyle = diff3
[github]
	user = fuentesjr
[push]
	default = simple
	autoSetupRemote = true
[init]
	defaultBranch = trunk
